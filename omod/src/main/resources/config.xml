<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">
	
    <!-- Module Properties -->
	<id>mohregister</id>
	<name>MOH Register Report Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.mohregister</package>
	<author>nribeka</author>
	<description>
			This module will provide cohort definition to generate the MOH Register.
	</description>
	
	<!-- / Module Properties -->

	<require_modules>
		<require_module version="0.6">org.openmrs.module.reporting</require_module>
	</require_modules>
	<require_version>1.8.0</require_version>


    <!-- Module Activator -->
	<activator>org.openmrs.module.mohregister.MOHRegisterReportActivator</activator>


	
	  	
  	<!-- Maps hibernate file's , if present -->   	
   	<mappingFiles>
		${omodHbmConfig}
	</mappingFiles>

	
	<!-- Internationalization -->
	<!-- All message codes should start with @MODULE_ID@.* -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->
	
</module>

